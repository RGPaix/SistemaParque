<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.PasswordField?>
<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TelaCadastroController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Label alignment="CENTER" layoutX="241.0" layoutY="55.0" style="&#10;" text="Cadastro de usuário" textAlignment="CENTER" wrapText="false">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <Label layoutX="83.0" layoutY="105.0" text="Nome:" />
                <Label layoutX="83.0" layoutY="150.0" text="CPF:" />
                <Label layoutX="82.0" layoutY="192.0" text="Endereço:" />
                <Label layoutX="83.0" layoutY="230.0" text="Telefone:" />
                <Label layoutX="83.0" layoutY="270.0" text="Crie uma senha:" />
                <TextField fx:id="textNome" layoutX="151.0" layoutY="101.0" prefHeight="25.0" prefWidth="390.0" promptText="Digite seu nome completo" />
                <TextField fx:id="textCPF" layoutX="151.0" layoutY="146.0" prefHeight="25.0" prefWidth="390.0" promptText="Digite o CPF sem pontos ou traços" />
                <TextField fx:id="textEndereco" layoutX="151.0" layoutY="188.0" prefHeight="25.0" prefWidth="390.0" promptText="Digite seu endereço completo, com a cidade" />
                <TextField fx:id="textTelefone" layoutX="151.0" layoutY="226.0" prefHeight="25.0" prefWidth="390.0" promptText="Exemplo: (xx)xxxxxxxxx" />
                <PasswordField fx:id="textSenha" layoutX="180.0" layoutY="266.0" prefHeight="25.0" prefWidth="361.0" />
                <Button fx:id="bttnSenha" layoutX="274.0" layoutY="330.0" mnemonicParsing="false" text="Fazer cadastro" onAction="#cadastrar"/>
            </children>
        </AnchorPane>
    </children>
</VBox>